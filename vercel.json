{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/view/app.html"
    },
    {
      "src": "/resources/(.*)",
      "dest": "/resources/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/view/app.html"
    }
  ]
} 
